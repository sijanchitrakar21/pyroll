<?xml version="1.0"?>
<odoo>
        <!-- Merge Opportunities  -->
        <record id="import_csv_timesheet_form" model="ir.ui.view">
            <field name="name">hr.timehsheet.import.form</field>
            <field name="model">hr.timesheet.import</field>
            <field name="arch" type="xml">
                <form >
				
				 <group >
                    <group >
                        <field name="csv_file" />
						
						<field name="paydate" required="1"/>
						<field name="payfrequency_id" required="1"/>
						
						 </group >
						 
						  <group >
						  <field name="xls_dcs" />
							<field name="date_from" required="1"/>
							<field name="date_to" required="1"/>
							
						   </group >
                      </group >
                    <footer>
                        <button name="action_import_csv" invisible="1" type="object" string="Start Import" class="btn-primary"/>
						<button name="action_import_new" type="object" string="Start Import" class="btn-primary"/>
					
						
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
 

</odoo>
