<?xml version="1.0" encoding="utf-8"?>
<odoo>


<template id="report_defaultpage_document"> 
	<t t-call="web.external_layout"> 
		
			<div class="page">
			<h3>
				<span>Report </span>
			</h3>
			 
			<h4>
				Date: <t if="date_from"><span t-esc="date_from" t-field-options='{"format": "dd/MM/yyyy"}'/></t> - <t if="date_till"><span t-esc="date_till" t-field-options='{"format": "dd/MM/yyyy"}'/></t>
			</h4>
			</div>
		 </t>
	 
</template>


<template id="report_defaultpage">
    <t t-call="web.html_container">
        <t t-foreach="docs" t-as="doc">
            <t t-call="dincelpayroll.report_defaultpage_document" />
        </t>
    </t>
</template>
 
 
</odoo>
