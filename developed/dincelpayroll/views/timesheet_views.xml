				<field name="x_round_stop" />
				<field name="x_break_paid" />
				<field name="x_hrs_normal" />
				<field name="x_break_unpaid" />
				<field name="x_loading_noon" />
				<field name="x_loading_night" />
			</field>
		 </field>
    </record>
	
