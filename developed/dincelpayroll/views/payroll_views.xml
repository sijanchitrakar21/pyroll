							<field name="x_emp_number" readonly="1" />
							<field name="x_job_id" readonly="1"/>
							<field name="x_group_id" readonly="1"/>
							<field name="x_emp_status" readonly="1"/>
							
							<field name="x_clearing_account_id" invisible="1" />
							<field name="x_payslip_deli" readonly="1"/>
							<field name="x_email_pay" readonly="1"/>
							<field name="x_time_import_id" readonly="1"/>
							
							<field name="x_printed" invisible="1"/>
							<field name="x_emailed" invisible="1"/>	
							<field name="x_xls_dcs" invisible="1"/>	
