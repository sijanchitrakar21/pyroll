								<button name="button_generate_ato_test" string="TEST XML" type="object" />	
								
								 
								
						</group>
					</group>
					
                </group>
			
                <notebook>
					<page string="Payslip">
					 
						<group colspan="2" >
						<field name="x_summary_ids" nolabel="1"   >
						<tree string="Summary" editable="bottom" >
							<field name="sequence" widget="handle" />
							<field name="pay_header" />
							<field name="category_id" />
							
							<field name="name" invisible="1"/>
							<field name="cost_state_id"  invisible="1" options="{'no_quick_create': True, 'no_create_edit' : True}"/>
							<field name="costcentre_id"  options="{'no_quick_create': True, 'no_create_edit' : True}"/>
							<field name="account_id"/>
							<field name="tot_hrs" sum="Sum Total" invisible="1" />
							<field name="factor_type"  invisible="1" />
							<field name="xfactor"  />
							<field name="pay_rate"  invisible="1"/>
							<field name="net_hrs" sum="Net Total"   />
							<field name="net_rate"  />
							<field name="tax_id" invisible="1" />
							<field name="sub_total" sum="Sum Subtotal" />
							<field name="ytd_total" readonly="1"/>
							<field name="super" readonly="1"/>
							<field name="taxable" readonly="1"/>
							<field name="is_summary" />
							<field name="is_manual" />
						</tree>
						
						</field>
						
						</group>
						
						<group colspan="2">
							<group>
								
								<field name="x_gross_ytd" widget="monetary"  readonly="1"/>
								<field name="x_tax_ytd"   widget="monetary" readonly="1"/>
								<field name="x_super_ytd"  widget="monetary" readonly="1"/>
								<field name="x_annual_ytd"  readonly="1"/>
								<field name="x_personal_ytd" readonly="1"/>
								<field name="x_lsl_ytd" readonly="1"  attrs="{'invisible': [('x_lsl_enable','=', False)]}"/>
								
							</group>
							
							<group class="oe_subtotal_footer oe_right" >
						  
									 
								<field name="x_gross_payslip_amt" widget="monetary"  readonly="1"/> 
								
								<field name="x_gross_amt" widget="monetary"  readonly="1"/>
								
								<field name="x_tax_amt" string="TAX" readonly="1"/>
								<field name="x_net_amt" readonly="1"/>
								 
								<field name="x_super_amt"  string="SUPER" readonly="1"/>
								
								<field name="x_annual_leave" readonly="1"/>
								<field name="x_sick_leave" readonly="1"/>
								<field name="x_lsl_leave" readonly="1"  attrs="{'invisible': [('x_lsl_enable','=', False)]}"/> 
								
								<field name="x_lsl_enable"   invisible="1"/>
							 
							
							</group>
							
							</group>
					</page>
					
					<page string="Timesheet Details">
						 <field name="x_timesheet_ids2"   context="{'default_employee_id': employee_id}">
							<tree string="Timesheet"  editable="bottom"  open="false">
								<field name="employee_id"  invisible="1"/>
								
								<field name="sequence"  widget="handle" />
								<field name="date" required="1" />
								<field name="name" required="1" />
								
								<field name="category_id" required="1" />
								<field name="accrued_leave" invisible="1" />
								
								<field name="hrs" sum="Tot. Hrs" required="1" />
								<field name="factor_type"  />
								<field name="xfactor"  required="1" />
								<field name="hrs_net"  invisible="1" sum="Net Hrs" />
								
								 
								
								
							</tree>
						</field>
					</page>	
					<page string="Timesheet Summary" invisible="1" attrs="{'invisible': [('x_is_timesheet','=', False)]}" >
                        <field name="x_timesheet_ids" >
							<tree string="Timesheet"  editable="bottom"  open="false">
								<field name="employee_id"  invisible="1"	 />
								<field name="name"  options='{"fg_color": "grey:is_weekend==True;white:is_over_hours==True;","bg_color":"red:is_over_hours==True;"}'/>
								<field name="date" options='{"fg_color": "grey:is_weekend==True;"}' />
								<field name="category_id" options='{"fg_color": "grey:is_weekend==True;"}' domain="[('type', '=', '0'),('category','=','wage')]"/>
								<field name="time_in" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="time_out" options='{"fg_color": "grey:is_weekend==True;"}'/>
								
								<field name="tot_hrs" invisible="1" sum="Total Hrs" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="break_unpaid" sum="Total Unpaid Break" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="hrs_normal" sum="Total Normal" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="break_paid" sum="Total Paid" options='{"fg_color": "grey:is_weekend==True;"}'/>
								
								<field name="hrs_t15" sum="Total T1.5" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="hrs_t20" sum="Total T2.0" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="account_id" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="project_id" options='{"fg_color": "grey:is_weekend==True;"}'/>
								
								<field name="leave_annual" sum="Total Annual" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="leave_sick" sum="Total Sick" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="leave_unpaid" sum="Total Unpaid" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="leave_part" sum="Total Part" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="rate_loading" invisible="1" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="hrs_loading" invisible="1" sum="Total Loading" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="loading_noon" sum="Total Loading Noon" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="loading_night" sum="Total Loading Night" options='{"fg_color": "grey:is_weekend==True;"}'/>
								<field name="tot_worked_hrs"   sum="Total Worked" options='{"fg_color": "grey:is_weekend==True;"}'/>
								
								<field name="is_weekend" invisible="1"/>
								<field name="is_over_hours" invisible="1"/>
                            </tree>
                        </field>
                    </page>
					
					<page string="Leaves">
						<button type="object" string="Update Leave Timesheet" name="button_update_leave_ts"/>
					   <group colspan="2">
					   
						<field name="x_leave_ids" editable="bottom"  context="{'default_employee_id': employee_id}" nolabel="1">
							<tree >
								<field name="category_id"   />
								<field name="holiday_status_id"  invisible="1" />
								<field name="holiday_id" invisible="1"  context="{'default_employee_id': parent.employee_id}"/>
								<field name="employee_id" invisible="1" />
								<field name="date_from"  />
								<field name="date_to"  />
								<field name="day_ids2"  invisible="1" widget="many2many_tags"/>
								<field name="date" invisible="1" />
								
								<field name="tot_hrs" sum="Tot Hours" />
							</tree>
							<form >
								<group>
								<group> <!-- 
									<field name="holiday_id" required="1" domain="[('employee_id','=',parent.employee_id),('x_date_from','&lt;=',parent.date_to),('x_date_from','&gt;=',parent.date_from)]"  context="{'default_employee_id': parent.employee_id}"/> -->
									<field name="holiday_id" required="1" domain="[('employee_id','=',parent.employee_id),('x_redeem','in',['part','none'])]"  context="{'default_employee_id': parent.employee_id}"/> 
									<field name="employee_id" invisible="1"  context="{'default_employee_id': parent.employee_id}"/>
									<field name="date_from" invisible="1"/>
									<field name="date_to"  invisible="1"/>
									
									<field name="date" invisible="1"/>
									
									</group>
									<group>
									
									<field name="category_id"  invisible="1"  />
									<field name="balance_hrs"  />
									<field name="tot_hrs" invisible="1" />
									
									<field name="day_ids2" invisible="1"  options="{'no_quick_create': True, 'no_create_edit' : True}" widget="many2many_tags" domain="[('payslip_id','=',parent.id)]"/>
									</group>
								</group>
								<group>
									<field name="leave_days"> 
										<tree editable="bottom" >
											<field name="date" invisible="1"   />
											<field name="name"   />
											<field name="is_leave" />
											<field name="hours" sum="Total Hours"  />
										</tree>	
									</field>
								</group>
							</form>
						</field>
					   
					   </group>
				   </page>
					
				
					<page  string="Journals">
						<separator string="Journals"/>
						<field name="x_journal_ids" readonly="1">
						 </field>
					</page>		
							
                    <page invisible="1"  string="Worked Days &amp; Inputs">
                        <separator string="Worked Days"/>
                        <field name="worked_days_line_ids">
                            <tree string="Worked Days" editable="bottom">
                                <field name="name"/>
                                <field name="code"/>
                                <field name="number_of_days" sum="Total Working Days"/>
                                <field name="number_of_hours"/>
                                <field name="contract_id"/>
                                <field name="sequence" invisible="True"/>
                            </tree>
                            <form string="Worked Day">
                                <group col="4">
                                    <field name="name"/>
                                    <field name="code"/>
                                    <field name="sequence"/>
                                    <field name="number_of_days"/>
                                    <field name="number_of_hours"/>
                                    <field name="contract_id"/>
                                </group>
                            </form>
                        </field>
                        <separator string="Other Inputs"/>
                        <field name="input_line_ids" colspan="4" nolabel="1">
                            <tree string="Input Data" editable="bottom">
                                <field name="name"/>
                                <field name="code"/>
                                <field name="amount"/>
                                <field name="contract_id"/>
                                <field name="sequence" invisible="True"/>
                            </tree>
                            <form string="Payslip Line">
                                <group col="4">
                                    <field name="name"/>
                                    <field name="code"/>
                                    <field name="sequence"/>
                                    <field name="amount"/>
                                    <field name="contract_id"/>
                                </group>
                            </form>
                        </field>
						
                    </page>
					
                    <page invisible="1" string="Salary Computation">
                        <field name="line_ids" colspan="4" nolabel="1">
                            <tree string="Salary Structure" editable="bottom" >
                                <field name="name"/>
                                <field name="code"/>
                                <field name="category_id"/>
                                <field name="sequence" invisible="1"/>
                                <field name="quantity"/>
                                <field name="rate"/>
                                <field name="salary_rule_id"/>
								<field name="contract_id"/>
                                <field name="amount"/>
                                <field name="total"/>
                            </tree>
                            <form string="Payslip Line">
                                <group col="4">
                                    <field name="name"/>
                                    <field name="code"/>
                                    <field name="category_id"/>
                                    <field name="sequence"/>
                                    <field name="quantity"/>
                                    <field name="rate"/>
                                    <field name="amount"/>
                                    <field name="total"/>
                                    <field name="salary_rule_id"/>
									<field name="contract_id"/>
                                </group>
                            </form>
                        </field>
                    </page>
                    <page invisible="1" string="Details By Salary Rule Category">
                        <field name="details_by_salary_rule_category" context="{'group_by':'category_id'}" domain="[('appears_on_payslip', '=', True)]">
                            <tree string="Payslip Lines" decoration-info="total == 0">
                                <field name="category_id"/>
                                <field name="name"/>
                                <field name="code"/>
                                <field name="total"/>
                            </tree>
                        </field>
                   </page>
                    <page invisible="1" string="Accounting Information">
                        <group>
                            <group string="Miscellaneous">
                                <field name="company_id" groups="base.group_multi_company"/>
                                <field name="payslip_run_id" domain="[('state','=','draft')]"/>
                            </group>
                            <group name="accounting" string="Accounting">
                                <field name="paid" readonly="1"/>
                            </group>
                        </group>
                        <div colspan="4">
                            <field name="note" placeholder="Internal note..."/>
                        </div>
                   </page>
                
				</notebook>
                  <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    
                    <field name="message_ids" widget="mail_thread"/>
                </div>
            </form>
		 </field>
    </record>
	
