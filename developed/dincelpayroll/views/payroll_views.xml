		<record id="dincel_payroll_timesheet_filter" model="ir.ui.view">
		<field name="name">Timesheet Search</field>
		<field name="model">hr.payslip.timesheet</field>
		<field name="arch" type="xml">
			<search>
					<field name="name" />
					<field name="date" />
					<field name="employee_id"/>
			
			 
				 
			  
					<group expand="0" string="Group By">
					<filter string="Employee"   domain="[]" help="Employee" context="{'group_by':'employee_id'}" />
					 
			 
				</group>
