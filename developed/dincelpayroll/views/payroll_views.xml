<?xml version="1.0" encoding="utf-8"?>
<odoo>
	 
	<record id="view_dincelpayroll_payslip_filter" model="ir.ui.view">
        <field name="name">dincelpayroll.payslip.select</field>
        <field name="model">hr.payslip</field>
        <field name="arch" type="xml">
            <search string="Search Payslips">
                <filter string="Hour Variance" domain="[('x_hours_variance','=',True)]" help="Hour Variance"/>
				<field name="name" string="Payslips" filter_domain="['|',('name','ilike',self),('number','ilike',self)]"/>
                <field name="date_from"/>
                <filter string="Draft" domain="[('state','=','draft')]" help="Draft Slip"/>
                <filter string="Done" domain="[('state','=','done')]" help="Done Slip"/>
				<filter string="Timesheet" domain="[('x_is_timesheet','=',True)]" help="Timesheet Slips"/>
				<filter string="Autopay/Salary" domain="[('x_is_timesheet','=',False)]" help="Autopay/Salary Slip"/>
				
                <field name="employee_id"/>
                <field name="payslip_run_id" invisible="1"/>
                <group expand="0" string="Group By">
                    <filter string="Employees" name="employee_id" context="{'group_by':'employee_id'}"/>
                    <filter string="PaySlip Batch" name="payslip_run_id" context="{'group_by':'payslip_run_id'}" invisible="1"/>
                    <filter string="Companies" name="company_id" groups="base.group_multi_company" context="{'group_by':'company_id'}"/>
                    <filter string="States" name="state" context="{'group_by':'state'}"/>
					<filter string="Timesheet" name="timesheet" context="{'group_by':'x_is_timesheet'}"/>
                </group>
            </search>
        </field>
    </record>
	
	
	
