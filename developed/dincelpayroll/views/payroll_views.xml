					<group>
						<field name="employee_id" readonly="1" string="Employee Name"/>
						<field name="date" string="Payment Date"  />
						<field name="x_payfrequency_id"  readonly="1"/>
						<field name="x_type" invisible="1"/>
						<label for="date_from" string="Pay Period" />
						<div>
							<field name="date_from" class="oe_inline"  /> - <field name="date_to" class="oe_inline"  />
