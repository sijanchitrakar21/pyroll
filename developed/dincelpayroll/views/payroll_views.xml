						<field name="number" string="Pay Reference ID on payslip"/>					
						<field name="x_chequeno" />
						<field name="contract_id" domain="[('employee_id','=',employee_id),('date_start','&lt;=',date_to),'|',('date_end','&gt;=',date_from),('date_end','=',False)]" context="{'default_employee_id': employee_id}" invisible="1"/>
					   
